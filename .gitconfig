[user]
	email = fre@example.com
	name = FRE

[credential]
	helper = cache --timeout 7200

[credential "https://github.com"]
	username = fpreynaud

[credential "https://bitbucket.org"]
	username = fpreynaud

[core]
	editor = vim
	whitespace = -blank-at-eol

[alias]
	logd = log --decorate=full --color=always
	tree = log --graph --decorate --oneline --branches=* --color=always
	diffl = diff HEAD^
	dif = diff --color=always --ignore-space-at-eol
	index = ls-files
	st = status
	repoconf = config -l -f .git/config
	newbranch = checkout -b
	worddiff = diff --color-words --color=always
	hist = log --format=format:\"%C(yellow)commit %H %gD%nAuthor: %aN <%aE>%nDate: %aD%n%n%s%n%n%b\" -g
	discard = reset --hard HEAD
	a = add
	b = branch
	c = commit
	ch = checkout
	d = diff
	l = log
	m = mergetool
	r = rebase
	s = status
	t = log --graph --decorate --oneline --branches=* --color=always

[push]
	default = matching

[color]
	status = always
	interactive = always
	diff = always
	branch = always
